{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "lib": ["dom"],
    "declaration": true,
    "declarationMap": true,
    "strict": false,
    "sourceMap": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "exclude": [],
  "baseUrl": ".",
}
